{
  "name": "htmlstring-to-react",
  "version": "5.3.1",
  "description": "Convert HTML DOM to React elements",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "prepare": "husky install",
    "build": "npm run clean && tsc --project .",
    "clean": "rimraf lib/ yarn-error.log npm-debug.log",
    "lint": "eslint --ext .ts .",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest",
    "start": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thomasthiebaud/htmlstring-to-react.git"
  },
  "author": "Thomas Thiebaud",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thomasthiebaud/htmlstring-to-react/issues"
  },
  "homepage": "https://github.com/thomasthiebaud/htmlstring-to-react#readme",
  "files": [
    "lib",
    "types"
  ],
  "devDependencies": {
    "@commitlint/cli": "17.6.7",
    "@commitlint/config-conventional": "17.6.7",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/react": "14.0.0",
    "@types/dompurify": "3.0.2",
    "@types/jest": "29.5.3",
    "@types/react": "18.2.17",
    "@typescript-eslint/eslint-plugin": "6.2.0",
    "@typescript-eslint/parser": "6.2.0",
    "conventional-changelog-eslint": "4.0.0",
    "eslint": "8.46.0",
    "eslint-config-prettier": "8.9.0",
    "eslint-plugin-prettier": "5.0.0",
    "husky": "8.0.3",
    "jest": "29.6.2",
    "jest-environment-jsdom": "29.6.2",
    "prettier": "3.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rimraf": "5.0.1",
    "semantic-release": "21.0.7",
    "ts-jest": "29.1.1",
    "typescript": "5.1.6"
  },
  "dependencies": {
    "dompurify": "2.3.6"
  },
  "peerDependencies": {
    "react": "^16.2.0 || ^17.0.0 || ^18.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "<rootDir>/tests/styleMock.js"
    },
    "testEnvironment": "jsdom"
  }
